class Solution {
public:
    int search(vector<int>& nums, int target) {
        // Write your code here
		int min=0;
		int max=nums.size()-1;
		while(min<=max){
		int mid=(min+max)/2;
		if(target==nums[mid]){
			return mid;
		}
		else if(target>nums[mid]){
			min=mid+1;
		}
		else{
			max=mid-1;
		}
		}
		return -1;
    }
};

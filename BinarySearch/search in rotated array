int search(vector<int>& arr, int key) {
    // Your code here
int start=0;
int end=arr.size()-1;
while(start<=end){
	int mid=start+(end-start)/2;
	if(key==arr[mid]){
		return mid;
	}
	else if(arr[mid]>=arr[0]){
	if(key>=arr[start]&&key<=arr[mid]){
		end=mid-1;
	}
	else{
		start=mid+1;
	}
	}
	else{
	if(key>=arr[mid]&&key<=arr[end]){
		start=mid+1;
	}
	else{
	    end=mid-1;
	}
	}
}
return -1;
}
